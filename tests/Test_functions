#### CVR ####
randata.CVR <- data.frame(
  obs1 = rep(3, 10),
  obs2 = sample(2:3, 10, replace = TRUE),
  obs3 = rep(3, 10),
  obs4 = c(rep(3, 7), rep(2, 3)),
  obs5 = rep(x = 2, times = 10, each = 1),
  obs6 = sample(1:3, 10, replace = TRUE))

